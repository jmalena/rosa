:set prompt "Î» "

:m +Rosa.AST
:m +Rosa.Compiler
:m +Rosa.Parser
:m +Rosa.PrettyPrinter

:{
parse' :: String -> IO ()
parse' s =
  case parse s of
    Left err -> error err
    Right ast -> putStrLn $ show ast

prettyPrint' :: String -> IO ()
prettyPrint' s =
  case parse s of
    Left err -> error err
    Right ast -> putStrLn $ prettyPrint ast

compile' :: String -> IO ()
compile' s =
  case compile s of
    Left err -> error err
    Right asm -> putStrLn asm
:}
