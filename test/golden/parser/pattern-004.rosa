--| Constructor pattern with arguments should parse correctly.

scott list : type -> type
list a :=
  (nil  : r) ->
  (cons : a -> list a -> r) ->
  r

cons nil : {a : type} -> list a
nil {a} := \nil _ => nil

cons cons : {a : type} -> a -> list a -> list a
cons {a} x xs := \_ cons => cons x xs

head : list a -> a
head (cons x _) := x