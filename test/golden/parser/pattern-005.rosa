--| Nested constructor pattern should parse correctly.

scott tree : type -> type
tree a :=
  (leaf : a -> r) ->
  (node : tree a -> a -> r) ->
  r

cons leaf : {a : type} -> a -> tree a
leaf {a} x := \leaf _ => leaf x

cons node : {a : type} -> tree a -> a -> tree a
node {a} left x := \_ node => node left x

value : tree a -> a
value (node (leaf x) y) := y